page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates .goods_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cates .goods_group .goods_title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.cates .goods_group .goods_title .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.cates .goods_group .goods_title .goods_list navigator {
  width: 50%;
  text-align: center;
}
.cates .goods_group .goods_title .goods_list navigator image {
  width: 100%;
}
