page {
  background-color: #fff;
}
.index_zhiding {
  padding: 10rpx 10rpx;
  display: flex;
}
.index_zhiding image {
  border-radius: 15rpx;
  align-items: center;
  justify-content: center;
  height: 100%;
  width: 100%;
}
.index_swiper swiper {
  width: 750rpx;
  height: 240rpx;
  padding: 10rpx 10rpx;
}
.index_swiper swiper image {
  width: 100%;
  border-radius: 15rpx;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  padding: 0rpx 10rpx;
  flex: 1;
}
.index_cate navigator .text {
  justify-content: center;
  color: dodgerblue;
  display: flex;
  font-size: 35rpx;
  font-weight: 900;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .floor_group {
  background-color: #dedede;
}
.index_floor .floor_group .floor_title {
  padding: 0 10rpx;
  border-top: 10rpx solid #dedede;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
  height: 400rpx;
  border-radius: 15rpx;
}
.index_floor .floor_group .floor_list {
  padding: 0rpx 10rpx;
}
.index_floor .floor_group .floor_list navigator {
  float: left;
  width: 50%;
  border-top: 10rpx solid #dedede;
}
.index_floor .floor_group .floor_list navigator:nth-child(2),
.index_floor .floor_group .floor_list navigator:nth-child(4),
.index_floor .floor_group .floor_list navigator:nth-child(6) {
  border-left: 20rpx solid #dedede;
  border-right: 10rpx solid #dedede;
}
.index_floor .floor_group .floor_list navigator:nth-child(1),
.index_floor .floor_group .floor_list navigator:nth-child(3),
.index_floor .floor_group .floor_list navigator:nth-child(5) {
  border-left: 10rpx solid #dedede;
}
.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 450rpx;
  border-radius: 20rpx;
}
